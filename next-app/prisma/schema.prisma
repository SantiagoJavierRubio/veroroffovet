generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider      = "mysql"
  url           = env("DATABASE_URL")
  relationMode  = "prisma"
}

model Price {
  title         String    @db.VarChar(255) @unique @id
  value         Int
  updatedAt     DateTime  @updatedAt @default(now())
}

model Curso {
  id            String    @unique @id @default(cuid())
  type          String
  title         String
  institution   String
  inCourse      Boolean   @default(false)
}